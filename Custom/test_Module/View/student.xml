<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="student_tree" model="ir.ui.view">
        <field name="name">student.data.tree</field>
        <field name="model">student.data</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="age"/>
                <field name="gender"/>
                <field name="note"/>

            </tree>
        </field>
    </record>

    <record id="student_form" model="ir.ui.view">
        <field name="name">student.data.form</field>
        <field name="model">student.data</field>
        <field name="arch" type="xml">
            <form>

            <sheet>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="age"/>
                    </group>
                    <group>
                        <field name="gender"/>
                        <field name="note"/>
                        <field name="classs_id"/>
                        <field name="subject_ids"/>
                        <field name="student"/>
                        <field name="student_id"/>

                    </group>
                </group>
                <field name="name"/>
            </sheet>
            </form>
        </field>
    </record>

    <record id="student_action" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">student.data</field>
        <field name="view_mode">tree,kanban,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            </p>
        </field>
    </record>

    <menuitem id="student_root"
              name="Student"
              sequence="10"/>

    <menuitem id="student_name_root"
              name="Students"
              parent="student_root"
              sequence="10"/>

    <menuitem id="student_name"
              name="Chokre"
              parent="student_name_root"
              action="student_action"
              sequence="10"/>
</odoo>